{
  "manifest_version": 3,
  "name": "Chrome Security Core",
  "version": "1.0",
  "description": "تأمين وحماية متصفح جوجل كروم من الاختراق",
  "permissions": ["tabs", "scripting", "storage", "webNavigation"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
